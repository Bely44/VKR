	$(VDSP)/cc218x.exe .\Debug\thesis.doj -T .\ASD.ldf -map .\Debug\aues.map.xml -flags-link -xref -flags-link -save-temps -L .\Debug -flags-link -od,.\Debug -o .\Debug\aues.dxe -proc ADSP-2181 -flags-link -MM

