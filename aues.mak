ifeq ($(MAKECMDGOALS),aues_Debug)

aues_Debug : ./Debug/aues.dxe 

./Debug/thesis.doj :./thesis.asm 
	$(VDSP)/easm218x.exe .\thesis.asm -proc ADSP-2181 -v -g -save-temps -sp -l .\Debug\thesis.lst -o .\Debug\thesis.doj -MM

